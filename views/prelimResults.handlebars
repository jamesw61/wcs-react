    <!-- Prelim Results View -->
    <div class="container">
	
      <!-- Jumbotron -->
      <div class="jumbotron">
        <h1 class="text-center"> {{round}} Results</h1>
        <h2 class="text-center">{{division}}  {{role}}</h2>
      </div>

      <div class="row">

        <div class="col-lg-12">


          	<div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title"><strong>Heat One</strong></h3>
            </div>
            <div class="panel-body">
              <table class="table table-hover" id='prelim-heatOne'>
                <thead>
                  <tr>
                    <th>Bib Number</th>
                    <th>Name</th>
                    <th>Judge 1</th>
                    <th>Judge 2</th>
                    <th>Judge 3</th>
                    <th>Total</th>
                  </tr>
                </thead>
              
                <tbody>
                	{{#each scores}}
                	<tr>
    				<td>{{this.bib_number}}</td>
    				<td>{{this.name}}</td>
    				<td>{{this.judge1}}</td>
            <td>{{this.judge2}}</td>
            <td>{{this.judge3}}</td>
            <td>{{this.total}}</td>
    				</tr>
  					{{/each}}

                </tbody>
                </table>
            
              
            </div>
          	</div>
          	</div>
        </div>

    </div>